public class deleteKth {
	public static int[] array = {0,2,4,6,1};
	public static LinkedList newList = new LinkedList();
	public static int K = 2;
	
	public static void main(String[] args){
		for(int i = 0; i < array.length; i++){
			int value = array[i];
			newList.add(value);
		}
		Node current = newList.head;
		for(int i = 0; i < K; i++){
			current = current.next;
		}
	}
}

class LinkedList{
	Node head;
	
	public LinkedList(){
	Node head = null;
	}
	
	public void add(int value){
		if(head == null){
			head.data = value;
		}
		else{
			head.next.data = value;
			head = head.next;
		}
	}
}

class Node{
	Node next;
	int data;
	
	public Node(int value){
		next = null;
		data = value;
	}
	
	public Node getNext(){
		return next;
	}
	
	public void setNext(int value){
		next.data = value;
	}
}


